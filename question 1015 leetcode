int smallestRepunitDivByK(int k) {
    /* n only contains the digit 1 
       k must be a positive number 
       n/k
    */
    if(k%2==0 || k%5==0) return -1;
    int remi=0;
    for(int i=1;i<=k;i++){
        remi=(remi*10+1)%k; // as can concists of the number only 1--------
        if(remi==0) return i;
    }
    return -1;
}
